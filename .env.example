NODE_ENV=development
PORT=3000

# Supabase Database Configuration
# Para usar Docker, comente as linhas Supabase e descomente as linhas Docker abaixo
DB_DIALECT=postgres
DB_HOST=YOUR_SUPABASE_HOST.supabase.co
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres.YOUR_PROJECT_REF
DB_PASS=YOUR_SUPABASE_PASSWORD

# Docker Database Configuration (descomente para usar Docker)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=leiloes
# DB_USER=postgres
# DB_PASS=postgres

SWAGGER_EXPAND=none

JWT_SECRET=dev-secret-key-change-in-production
JWT_EXPIRES_IN=1d

# Supabase Storage Configuration
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
